<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.rahul.farmerproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo22"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Item"></activity>
        <activity android:name=".BuyerDashboard" />
        <activity android:name=".ImagesActivity" />
        <activity android:name=".AddProducts" />
        <activity android:name=".FarmerMyOrders" />
        <activity android:name=".FarmerAddProduct" />
        <activity android:name=".FarmerProfile" /> <!-- tools:node="replace"> -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".FarmerDashboard" />
        <activity android:name=".FarmerRegisterActivity" />
        <activity android:name=".BuyerRegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>